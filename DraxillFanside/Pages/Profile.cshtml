@page
@model DraxillFanside.Pages.ProfileModel
@{
    ViewData["Title"] = "Profile";
}

<h2>Profile</h2>

@if (Model.CurrentUser != null)
{
    <div>
        <p>Username: @Model.CurrentUser.Username</p>
        <p>Role: @Model.UserRole</p>
    </div>
}
else
{
    <p>User not found or not logged in</p>
}
